{
    "containerDefinitions": [
        {
            "name": "clocktime-app",
            "image": "clocktime:latest",
            "portMappings": [
                {
                    "name": "clocktime-5000-tcp",
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/fargate-clocktime-app",
                    "awslogs-create-group": true,
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "fargate-clocktime",
    "executionRoleArn": "arn:aws:iam::730866387406:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}